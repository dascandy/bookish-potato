
template: __builtin_clang
compiler: clang++ --sysroot=sysroot --target=aarch64-unknown-linux-gnu 
compiler-cpp: clang++ -std=c++2a --sysroot=sysroot --target=aarch64-unknown-linux-gnu -fno-rtti -fno-exceptions -Icpp/include
# -mcpu=cortex-a72 -mfpu=crypto-neon-fp-armv8 -mfloat-abi=hardfp -fno-rtti -fno-exceptions
linker: ld.lld -T kernel/src/link.ld
archiver: ar
cross: true


