ENTRY(_start)
 
SECTIONS
{
	. = 1M;
 
	.text : ALIGN(4096)
	{
		*(.multiboot)
		*(.text.boot)
		*(.text.*)
	}
 
	/* Read-only data. */
	.rodata : ALIGN(4096)
	{
		*(.rodata.*)
	}
 
	.data : ALIGN(4096)
	{
		*(.data.*)
	}
  __bss_start = .;
	.bss : ALIGN(4096)
	{
		*(.bss.*)
		*(COMMON)
	}
  __bss_size = . - __bss_start;
}
